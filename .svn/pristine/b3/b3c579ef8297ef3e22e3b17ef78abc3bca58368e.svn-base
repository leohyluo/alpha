package com.alpha.self.diagnosis.pojo.vo;

import java.io.Serializable;
import java.util.List;

import com.alpha.self.diagnosis.pojo.BasicQuestion;

public class BasicQuestionVo implements Serializable {

	private static final long serialVersionUID = 6688903127327118326L;

	private int diagnosisId;
	
	private String displayType;
	
	private String questionTitle;
	
	/**
     * 问题编码
     */
    private String questionCode;
    
    private List<BasicAnswerVo> answers;
    
    public BasicQuestionVo(int diagnosisId, BasicQuestion basicQuestion, List<BasicAnswerVo> answers) {
    	this.diagnosisId = diagnosisId;
    	this.displayType = basicQuestion.getDisplayType();
    	this.questionCode = basicQuestion.getQuestionCode();
    	this.questionTitle = basicQuestion.getTitle();
    	this.answers = answers;
    }

	public int getDiagnosisId() {
		return diagnosisId;
	}

	public void setDiagnosisId(int diagnosisId) {
		this.diagnosisId = diagnosisId;
	}

	public String getDisplayType() {
		return displayType;
	}

	public void setDisplayType(String displayType) {
		this.displayType = displayType;
	}

	public String getQuestionCode() {
		return questionCode;
	}

	public void setQuestionCode(String questionCode) {
		this.questionCode = questionCode;
	}

	public List<BasicAnswerVo> getAnswers() {
		return answers;
	}

	public void setAnswers(List<BasicAnswerVo> answers) {
		this.answers = answers;
	}

	public String getQuestionTitle() {
		return questionTitle;
	}

	public void setQuestionTitle(String questionTitle) {
		this.questionTitle = questionTitle;
	}
}
