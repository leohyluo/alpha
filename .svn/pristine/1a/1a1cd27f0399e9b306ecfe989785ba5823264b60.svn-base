package com.alpha.user.pojo.vo;

import java.io.Serializable;

import com.alpha.user.controller.req.vo.PatientBasicInfo;

/**
 * 问诊前的授权获取问诊号响应结果
 * @author Administrator
 *
 */
public class AuthVo implements Serializable {

	private static final long serialVersionUID = -207746704555659309L;

	//诊断号唯一编码
	private int diagnosisId;
	
	//用户（成员）唯一编码
	private Long userId;
	
	public AuthVo(PatientBasicInfo patientInfo) {
		this.diagnosisId = 1;
		this.userId = patientInfo.getUserInfo().getUserId();
	}

	public int getDiagnosisId() {
		return diagnosisId;
	}

	public void setDiagnosisId(int diagnosisId) {
		this.diagnosisId = diagnosisId;
	}

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}
}
