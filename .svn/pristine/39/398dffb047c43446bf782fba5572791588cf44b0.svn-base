package com.alpha.user.service.impl;

import java.util.List;

import javax.annotation.Resource;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.stereotype.Service;

import com.alpha.server.rpc.user.pojo.UserMember;
import com.alpha.user.mapper.UserMemberMapper;
import com.alpha.user.service.UserMemberService;

@Service
public class UserMemberServiceImpl implements UserMemberService {

	private Logger logger = LoggerFactory.getLogger(getClass());
	
	@Resource
	private UserMemberMapper mapper;
	
	
	@Override
	public List<UserMember> find(UserMember userMember) {
		logger.info("findByUserId.userId is {}", userMember.getUserId());
		List<UserMember> list = mapper.find(userMember);
		return list;
	}

}
