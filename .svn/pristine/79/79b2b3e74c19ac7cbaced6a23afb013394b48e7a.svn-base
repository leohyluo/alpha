public enum WomenSpecialPeriod {

	MENSTRUAL_PERIOD(1),		//月经期
    PREPARE_PREGNANCY(2),		//备孕中
    IN_PREGNANCY(3),			//妊娠期
    LOCTATION_PERIOD(4);		//哺乳期

    private Integer value;

    WomenSpecialPeriod(Integer value) {
        this.value = value;
    }

    public Integer getValue() {
        return value;
    }

    public void setValue(Integer value) {
        this.value = value;
    }
}
